{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": ["LOCALAPPDATA"],
  "tasks": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "eslint": {
      "outputs": []
    },
    "eslint:fix": {
      "outputs": []
    },
    "watch:link": {
      "cache": false
    }
  }
}
